{
    "manifest_version": 3,
    "name": "Quiz Whizz",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png",
      "default_popup_width": 400
    }
  }
  